        }
      }
    },
    "/dashboard/onsubmit": {
      "post": {
        "summary": "Dashboard Onsubmit",
        "description": "Handle dashboard actions (e.g., updating preferences)",
        "operationId": "dashboard_onsubmit_dashboard_onsubmit_post",
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "additionalProperties": true,
                "type": "object",
                "title": "Action Data"
              }
            }
          },
          "required": true
        },
        "responses": {
          "200": {
            "description": "Successful Response",
