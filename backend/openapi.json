    "/dashboard/onload": {
      "get": {
        "summary": "Dashboard Onload",
        "description": "Gather all data needed for dashboard display.\nSingle endpoint to minimize frontend API calls.",
        "operationId": "dashboard_onload_dashboard_onload_get",
        "responses": {
          "200": {
            "description": "Successful Response",
